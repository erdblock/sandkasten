title = user.username
extends ../layout.jade

block body
	section(class="col-lg-offset-1 col-lg-3  col-sm-4")
		- if(publisherInstances.length > 0){
			h2 Publisher
			ul(class="nav nav-pills nav-stacked")
				+publishers
		- }
		h2 New Publisher
		ul(class="nav nav-pills nav-stacked")
			- for (var key in publishers) {
				+navElement("/publishers/new/"+publishers[key].id, publishers[key].title)
			- }
	section(class="col-lg-7 col-sm-8")
		block main
